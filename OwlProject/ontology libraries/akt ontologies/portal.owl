  <owl:Class rdf:ID="Project">
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf rdf:resource="#Activity"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="#Event"/>
              <owl:Class rdf:about="#Technology"/>
              <owl:Class rdf:ID="Method"/>
              <owl:Class rdf:about="#Organization"/>
              <owl:Class rdf:about="#Information-Bearing-Object"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="produces-output"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="involves-organization"/>
        </owl:onProperty>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minCardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="has-project-member"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Transportation-Device">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Something tangible designed to transport people, animals, objects from A to B. For instance a bycicle, a car, a boat, etc.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.aktors.org/ontology/support#Tangible-Thing"/>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Generalised-Means-Of-Transport"/>
    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:Class>
  <owl:Class rdf:ID="Village">
