<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:support="http://www.aktors.org/ontology/support#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:dct="http://purl.org/dc/terms/"
    xmlns:portal="http://www.aktors.org/ontology/portal#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.aktors.org/ontology/portal">
  <owl:Ontology rdf:about="">
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >2.0</owl:versionInfo>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >AKT Reference Ontology (Portal Ontology)</rdfs:label>
    <owl:imports rdf:resource="http://www.aktors.org/ontology/support"/>
    <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Enrico Motta (OCML version)</dc:creator>
    <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Nick Gibbins (OWL translation)</dc:creator>
    <dc:title xml:lang="en">AKT Reference Ontology (Portal Ontology)</dc:title>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >2003-02-10</dct:created>
    <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >AKT Project</dc:creator>
    <dc:description xml:lang="en">The AKT Reference Ontology has been designed to support the AKT-2 demonstrator ("AKTive Portal"), and subsequent activities. The ontology, designed by the AKT-2 group and codified by Enrico Motta, extends Version 1.</dc:description>
  </owl:Ontology>
  <owl:Class rdf:ID="Software-Status">
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf rdf:resource="http://www.aktors.org/ontology/support#Intangible-Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="Distance-Teaching-University">
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="University"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Technology">
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf rdf:resource="http://www.aktors.org/ontology/support#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="Generic-Agent"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="has-author"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >By technology we mean engineered applications of science. I guess we are probably confining ourselves to tangible things but as I am not sure I will use thing as the direct superclass - e.g., an algorithm is an intangible thing, but it could be seen as a technology, if we give a broad interpretation of the term</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Academic-Support-Unit">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Educational-Organization-Unit"/>
    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:Class>
  <owl:Class rdf:ID="Postal-Address">
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Address"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Modified to allow addresses to be given as strings, with no structure</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Activity">
