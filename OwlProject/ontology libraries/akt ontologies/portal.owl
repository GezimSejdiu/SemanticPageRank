    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Working-Person"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Recorded-Video">
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Information-Bearing-Object"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Any kind of recorded video, which is tangible.  This also includes a mpeg file on a machine</rdfs:comment>
